{
    "name": "DevTools Sample Extension",
    "description": "A Basic DevTools Extension Interacting with the Inspected Page",
    "manifest_version": 3,
    "version": "1.0",
    "devtools_page": "devtools.html",
    "content_scripts": [{
        "matches": [
          "http://*/*",
          "https://*/*"
        ],
        "run_at": "document_idle",
        "js": [
          "content_script.js"
        ]
    }],
    "background": {
        "service_worker": "background.js"
    }
}
